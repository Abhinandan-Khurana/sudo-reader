{
    "name": "@unclutter/serverless-import",
    "version": "0.1.0",
    "private": true,
    "main": "index.js",
    "source": "src/index.ts",
    "targets": {
        "default": {
            "source": "src/index.ts",
            "outputFormat": "commonjs",
            "context": "node",
            "isLibrary": true,
            "includeNodeModules": [
                "@unclutter/heatmap"
            ],
            "optimize": false,
            "sourceMap": false
        }
    },
    "scripts": {
        "build": "parcel build && cp package.json dist/",
        "deploy": "yarn build && gcloud beta functions deploy serverless-import --gen2 --source=dist --entry-point=main --allow-unauthenticated --trigger-http --runtime=nodejs16 --region=us-east1 --memory=2048MB --timeout=120s"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "^3.1.3",
        "@mozilla/readability": "^0.4.2",
        "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
        "@tensorflow/tfjs-node": "4.1.0",
        "axios": "^1.2.2",
        "express": "^4.18.2",
        "jsdom": "^21.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "@types/jsdom": "^20.0.1",
        "parcel": "^2.8.2",
        "typescript": "^4.4.4"
    }
}
