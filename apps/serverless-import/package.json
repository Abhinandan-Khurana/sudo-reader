{
    "name": "@unclutter/serverless-import",
    "version": "0.1.0",
    "private": true,
    "main": "index.js",
    "source": "src/index.ts",
    "targets": {
        "default": {
            "source": "src/index.ts",
            "outputFormat": "commonjs",
            "context": "node",
            "isLibrary": true,
            "includeNodeModules": [
                "@unclutter/heatmap"
            ],
            "optimize": false,
            "sourceMap": false
        }
    },
    "scripts": {
        "build": "parcel build && cp package.json dist/",
        "deploy": "yarn build && gcloud beta functions deploy serverless-import --gen2 --source dist --entry-point main --allow-unauthenticated --trigger-http --runtime nodejs16 --region=us-east1"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "^3.1.3",
        "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
        "@tensorflow/tfjs": "^4.2.0",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "parcel": "^2.8.2",
        "typescript": "^4.4.4"
    }
}
