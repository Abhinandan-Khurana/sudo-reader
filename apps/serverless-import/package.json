{
    "name": "@unclutter/serverless-import",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "start": "yarn build && npx @google-cloud/functions-framework --target=serverless-import",
        "deploy": "yarn build && gcloud beta functions deploy serverless-import --gen2 --source dist --entry-point helloHttp --allow-unauthenticated --trigger-http --runtime nodejs16 --region=us-east1"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "^1.9.0",
        "express": "^4.18.2"
    }
}
